@inherits Umbraco.Web.Mvc.UmbracoViewPage<ContentModels.Home>
@using ContentModels = Umbraco.Web.PublishedModels;
@{
    Layout = "Master.cshtml";
    var isEnglish = UmbracoContext.VariationContextAccessor?.VariationContext?.Culture == "en-US";

}

@* the fun starts here *@

@Html.Partial("HomeBanner", Model)

@Html.Partial("HomeCarousel", Model)

@Html.Partial("WhoAreWe", Model)

@Html.Partial("Announcements", Model)

@Html.Partial("FAQs", Model)

@Html.Partial("Testimonials1", Model)

<!-- Start Enroll Area -->
<section class="enroll-area ptb-100">
    <div class="container">
        <div class="row">
            <div class="col-lg-5">
                <div class="enroll-wrap">
                    <form class="courses-form">
                        <span>@Umbraco.GetDictionaryValue("AnyQuestions")</span>
                        <h3>@Umbraco.GetDictionaryValue("ContactUsNow")</h3>

                        <div class="form-group">
                            <input type="text" class="form-control" id="Name" placeholder="@Umbraco.GetDictionaryValue("YourName")">
                        </div>

                        <div class="form-group">
                            <input type="email" class="form-control" id="email" placeholder="@Umbraco.GetDictionaryValue("YourEmail")">
                        </div>

                        <div class="form-group">
                            <input type="text" class="form-control" id="Number" placeholder="@Umbraco.GetDictionaryValue("PhoneNumber")">
                        </div>

                        <div class="form-group">
                            <textarea type="text" class="form-control" id="Number" rows="3" placeholder="@Umbraco.GetDictionaryValue("YourQuestion")"></textarea>
                        </div>

                        <button type="submit" class="default-btn">
                            @Umbraco.GetDictionaryValue("Submit")
                        </button>
                    </form>
                </div>
            </div>

            <div class="col-lg-7">
                <div class="enroll-img">
                    <img src="assets/img/enroll-img.png" alt="Image">
                </div>
            </div>
        </div>
    </div>
</section>
<!-- End Enroll Area -->
